<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crud_proyecto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TransporteActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ShoppingActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestauranteActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".OtrosActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".NaturalezaActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".MuseoActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".MonumentoActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".LuxuryActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".InformacionActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".AlojamientoActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".PlayasActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>